<%- include("partials/header") -%>
<h1>Home</h1>
<p><%= startingContent %></p>
<hr>
    <% posts.forEach(function(post) { %>
        <h3><%= post.title%></h3> 
        <p><%= post.content %></p>
        <p><%= post.id %></p>
        <form method="post" action="/delete">
            <button class="btn btn-danger" value="<%= post.id %>" name="postId" type="submit">Delete</button>
        </form>
    <% }) %>
<%- include("partials/footer") -%>